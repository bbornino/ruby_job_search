<%= form_with model: @job_site do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
<p>
    <%= form.label :url %><br>
    <%= form.text_field :url %>
  </p>

 <p>
    <%= form.label :description %><br>
    <%= form.text_area :description , cols: 50, rows: 10 %>
  </p>

  <p>
    <%= form.label :review_text %><br>
    <%= form.text_area :review_text , cols: 50, rows: 10 %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>